  "scripts": {
    "rm": "rm -rf node_modules && rm yarn.lock || rd /s /q node_modules",
    "yrn:update": "yarn upgrade-interactive --latest",
    "i": "yarn install",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build && rm -rf static && mv ./build/* ./ && rm -rf build && yarn run git:run",
    "test": "react-app-rewired test",
    "eject": "react-app-rewired eject",
    "------------------ git ------------------": "",
    "git:add": "git add .",
    "git:commit": "git commit -a -m 'auto'",
    "git:push": "git push",
    "git:run": "yarn run git:add && yarn run git:commit && yarn run git:push"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
